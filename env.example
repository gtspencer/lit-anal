# LangSmith Configuration
# Set LANGSMITH_TRACING=true to enable tracing
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=book-influence-dev

# OpenAI Configuration
# Required for LLM nodes (EntityRosterUpdate, InfluenceExtractor, BookSynthesis, Ranker)
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
# Default model used by all LLM nodes (can be overridden per node)
OPENAI_MODEL=gpt-4o-mini

# Node-specific model overrides (optional, falls back to OPENAI_MODEL)
ENTITY_ROSTER_MODEL=
ENTITY_ROSTER_TEMPERATURE=0.1
INFLUENCE_EXTRACTOR_MODEL=
INFLUENCE_EXTRACTOR_TEMPERATURE=0.2
BOOK_SYNTHESIS_MODEL=
BOOK_SYNTHESIS_TEMPERATURE=0.2
RANKER_MODEL=
RANKER_TEMPERATURE=0.1

# Environment
ENV=dev

